{
  "name": "sample-one-shot-payment",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./jsconfig.json --watch",
    "prepare": "npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark",
    "pretty": "prettier --write \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json}\""
  },
  "devDependencies": {
    "@material/typography": "^14.0.0",
    "@smui-extra/accordion": "^7.0.0-beta.15",
    "@smui-extra/badge": "^7.0.0-beta.15",
    "@smui/banner": "^7.0.0-beta.15",
    "@smui/button": "^7.0.0-beta.15",
    "@smui/card": "^7.0.0-beta.15",
    "@smui/dialog": "^7.0.0-beta.15",
    "@smui/layout-grid": "^7.0.0-beta.15",
    "@smui/snackbar": "^7.0.0-beta.15",
    "@smui/tooltip": "^7.0.0-beta.15",
    "@smui/top-app-bar": "^7.0.0-beta.15",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/adapter-static": "^2.0.3",
    "@sveltejs/adapter-vercel": "^3.0.3",
    "@sveltejs/kit": "^1.20.4",
    "prettier": "^3.0.3",
    "prettier-plugin-svelte": "^3.0.3",
    "sass": "^1.63.6",
    "scss": "^0.2.4",
    "smui-theme": "^7.0.0-beta.15",
    "svelte": "^4.0.5",
    "svelte-add": "^2023.9.17-0.0",
    "svelte-check": "^3.4.3",
    "typescript": "^5.0.0",
    "vite": "^4.4.2",
    "vite-plugin-mkcert": "^1.16.0"
  },
  "type": "module",
  "dependencies": {
    "@supabase/supabase-js": "^2.38.1",
    "svelte-copy-to-clipboard": "^0.2.5"
  }
}
